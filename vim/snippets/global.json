{
  "date": {
    "prefix": "date",
    "body": ["${CURRENT_YEAR}-${CURRENT_MONTH}-${CURRENT_DATE}"],
    "description": "Put the date in (Y-m-D) format"
  },

  "For Loop": {
    "prefix": ["for", "for-const"],
    "body": ["for (const ${2:element} of ${1:array}) {", "\t$0", "}"],
    "description": "A for loop."
  },

  "double-quote pair": {
    "prefix": "\"",
    "body": ["\"$1\"$0"]
  },

  "->": {
    "prefix": "l",
    "body": ["-> $0"]
  },

  "<-": {
    "prefix": "h",
    "body": ["<- $0"]
  },

  "=>": {
    "prefix": "L",
    "body": ["=> $0"]
  },

  "<=": {
    "prefix": "H",
    "body": ["<= $0"]
  },

  "single-quote pair": {
    "prefix": "'",
    "body": ["'$1'$0"]
  },

  "backtick pair": {
    "prefix": "`",
    "body": ["`$1`$0"]
  },

  "paren pair": {
    "prefix": "()",
    "body": ["($1)$0"]
  },

  "multiline paren pair": {
    "prefix": "())",
    "body": [
      "($0",
      ")"
    ]
  },

  "multiline paren pair with semicolon": {
    "prefix": "());",
    "body": [
      "($0",
      ");"
    ]
  },

  "curly-brace pair": {
    "prefix": "{}",
    "body": ["{$1}$0"]
  },

  "curly-brace pair with spaces": {
    "prefix": "{}-",
    "body": ["{ $1 }$0"]
  },

  "multiline curly brace pair": {
    "prefix": "{}}",
    "body": [
      "{$0",
      "}"
    ]
  },

  "multiline curly brace pair with comma": {
    "prefix": "{}},",
    "body": [
      "{$0",
      "},"
    ]
  },

  "multiline curly brace pair with semicolon": {
    "prefix": "{}};",
    "body": [
      "{$0",
      "};"
    ]
  },

  "multiline curly brace pair with closing paren": {
    "prefix": "{}})",
    "body": [
      "{$0",
      "})"
    ]
  },

  "multiline curly brace pair with closing paren and semicolon": {
    "prefix": "{}});",
    "body": [
      "{$0",
      "});"
    ]
  },

  "square bracket pair": {
    "prefix": "[]",
    "body": ["[$1]$0"]
  },

  "multiline square bracket pair": {
    "prefix": "[]]",
    "body": [
      "[$0",
      "]"
    ]
  },

  "multiline square bracket pair with semicolon": {
    "prefix": "[]];",
    "body": [
      "[$0",
      "];"
    ]
  },

  "double-curly": {
    "prefix": "{{",
    "body": ["{{$1}}$0"]
  },

  "double-curly with spaces": {
    "prefix": "{{-",
    "body": ["{{ $1 }}$0"]
  },

  "my name": {
    "prefix": "me",
    "body": ["Brett Rasmussen $0"]
  }
}
